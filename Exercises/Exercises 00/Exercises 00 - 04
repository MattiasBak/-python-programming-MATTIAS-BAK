import math
#y=K*x+m

Y=input('What is delta Y?\n> ')
X=input('What is delta X?\n> ')
K= int(Y)/int(X)
M = input(float('Where does the line cross the X axis?\n> '))
M = float(M)
print(f'The result is y={K}*X+{M}')

#Eller

A= (4,4)
B=(0,1)
k=(A[1]-B[1])/(A[0]-B[0])

print(f'The solution is: y={k}*X+1')
